<%# <% provide(:title, t('view.properties.show.title')) %> %>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Content</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @properties.each do |property| %>
      <tr>
        <td><%= property.property_type %></td>
        <td><%= property.url %></td>
        <td><%= property.other %></td>
        <td><%= property.trader %></td>
        <td><%= property.price %></td>
     </tr>
    <% end %>
  </tbody>
</table>
